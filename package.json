{
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "commit": "git cz",
    "dev": "vite --host --port 8080",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:format": "prettier . --write",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix",
    "lint": "npm run lint:format && npm run lint:fix"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.10",
    "@types/node": "^18.7.18",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.0",
    "react-leaflet": "^4.0.2",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.4.0",

    "socket.io-client": "^4.6.1",
    "type-fest": "^1.2.0",
    "websocket": "^1.0.34",
    "zod": "^3.20.2",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@vitejs/plugin-react": "^2.1.0",
    "concurrently": "^7.6.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.3",
    "pre-commit": "^1.2.2",
    "prettier": "2.7.1",
    "sass": "^1.57.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaniilKimlb/TimeChat_Frontend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DaniilKimlb/TimeChat_Frontend/issues"
  },
  "pre-commit": {
    "run": [
      "lint"
    ]
  },
  "homepage": "https://github.com/DaniilKimlb/TimeChat_Frontend#readme",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/git/commitizen.cjs"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g './config/git/commitlint.cjs'"
    }
  }
}
